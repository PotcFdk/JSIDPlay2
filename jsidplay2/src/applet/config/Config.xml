<!-- http://www.swixml.org - Swing UI XML based -->
<!-- Configuration -->
<panel layout="GridBagLayout" bundle="applet.collection.CollectionTexts">
	<scrollpane>
		<gridbagconstraints gridx="0" gridy="0" weightx="1"
			gridheight="1" weighty="1" fill="GridBagConstraints.BOTH" />
		<tree initclass="applet.config.ConfigModel" ExpandsSelectedPaths="true"
			ScrollsOnExpand="true" id="configTree" />
	</scrollpane>
	<panel>
		<gridbagconstraints gridx="1" gridy="0" weightx="1"
			gridheight="1" weighty="1" fill="GridBagConstraints.BOTH" />
	</panel>
</panel>