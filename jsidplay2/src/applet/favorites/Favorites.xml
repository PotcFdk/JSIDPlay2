<!-- http://www.swixml.org - Swing UI XML based -->
<!-- Play List -->
<panel xmlns="http://www.swixml.org/2007/Swixml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.swixml.org/2007/Swixml http://kenchis.t15.org/jsidplay2//swixml2-schema-2.6-SNAPSHOT.xsd"
	layout="BorderLayout" bundle="applet.favorites.FavoritesTexts">
	<panel layout="GridLayout(3, 1)" constraints="BorderLayout.NORTH">
		<label text="HINT" font="Georgia-ITALIC-12"></label>
		<label text="HINT2" font="Georgia-ITALIC-12"></label>
		<panel layout="BorderLayout">
			<label text="REG_EXPR_FILTER" font="Georgia-ITALIC-12"
				constraints="BorderLayout.WEST"></label>
			<textfield constraints="BorderLayout.CENTER" id="filterField"></textfield>
		</panel>
	</panel>
	<scrollpane constraints="BorderLayout.CENTER">
		<favoritestable id="favoritesTable" initclass="applet.favorites.FavoritesModel" />
	</scrollpane>
	<vbox constraints="BorderLayout.EAST">
		<NiceButton ToolTipText="UNSORT_FAVORITES" ActionCommand="Unsort"
			PreferredSize="20,20" Enabled="false" id="unsort" icon="applet/icons/unsort.png"
			action="doUnsort" />
		<NiceButton ToolTipText="MOVE_UP_FAVORITE" ActionCommand="Uo"
			PreferredSize="20,20" Enabled="false" id="moveUp" icon="applet/icons/up.png"
			action="doMoveUp" />
		<NiceButton ToolTipText="MOVE_DOWN_FAVORITE" ActionCommand="Down"
			PreferredSize="20,20" Enabled="false" id="moveDown" icon="applet/icons/down.png"
			action="doMoveDown" />
	</vbox>
</panel>