<!-- http://www.swixml.org - Swing UI XML based -->
<!-- About dialogue -->
<frame xmlns="http://www.swixml.org/2007/Swixml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.swixml.org/2007/Swixml http://kenchis.t15.org/jsidplay2//swixml2-schema-2.6-SNAPSHOT.xsd"
	bundle="applet.about.AboutTexts" title="TITLE" layout="BorderLayout"
	defaultCloseOperation="JFrame.DISPOSE_ON_CLOSE" size="1024,768"
	visible="true" iconImage="ICON">
	<scrollpane constraints="BorderLayout.CENTER" border="EtchedBorder"
		id="scroller">
		<textarea editable="false" lineWrap="false" highlighter="null"
			border="EmptyBorder(4,4,4,4)" id="credits" />
	</scrollpane>
	<panel constraints="BorderLayout.SOUTH">
		<button text="OK" id="ok"></button>
	</panel>
</frame>