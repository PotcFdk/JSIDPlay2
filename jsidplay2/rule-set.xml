<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE xml>
<ruleset comparisonMethod="maven"
	xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0
                             http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
	<ignoreVersions>
		<!-- ignore alphas, betas, release candidates, milestones and early access versions -->
		<ignoreVersion type="regex">.*[.-][Aa]lpha\d*</ignoreVersion>
		<ignoreVersion type="regex">.*[.-][Bb]eta\d*</ignoreVersion>
		<ignoreVersion type="regex">.*-b\d*\.\d*</ignoreVersion>
		<ignoreVersion type="regex">.*-[Rr][Cc]\d*</ignoreVersion>
		<ignoreVersion type="regex">.*-M\d*</ignoreVersion>
		<ignoreVersion type="regex">.*-ea\+\d*</ignoreVersion>
	</ignoreVersions>

</ruleset>